<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC 
		"-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    	"http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>userLoginServiceImp</servlet-name>
  	<servlet-class>com.tangledcode.lang8.server.service.UserLoginServiceImp</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>userLoginServiceImp</servlet-name>
  	<url-pattern>/lang8/login</url-pattern>
  </servlet-mapping>

</web-app>
