			 xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel styleName="content">
		<h2 ui:field="username"></h2>
		<p>
			<g:Label text="Email:" />
			<span ui:field="email" />
		</p>
	</g:HTMLPanel>
</ui:UiBinder> 