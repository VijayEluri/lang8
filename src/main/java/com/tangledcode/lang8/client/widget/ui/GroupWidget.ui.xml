<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<g:HTMLPanel styleName="content">
	
		<h3>Groups</h3>
		
		<p>
			<p>
				<g:Label text="Title:" />
				<g:Label ui:field="groupTitle_1" /><br />
			</p>
			<p>
				<g:Label text="Description:" />
				<span ui:field="groupDescribtion_1" /><br />
			</p>
			<p>
				<g:Label text="Founder:" />
				<g:Label ui:field="groupUser_1" /><br />
			</p>
		</p>
		
		<hr />
		
		<p>
			<p>
				<g:Label text="Title:" />
				<g:Label ui:field="groupTitle_2" /><br />
			</p>
			<p>
				<g:Label text="Description:" />
				<span ui:field="groupDescribtion_2" /><br />
			</p>
			<p>
				<g:Label text="Founder:" />
				<g:Label ui:field="groupUser_2" /><br />
			</p>
		</p>
		
		<hr />
		
		<p>
			<p>
				<g:Label text="Title:" />
				<g:Label ui:field="groupTitle_3" /><br />
			</p>
			<p>
				<g:Label text="Description:" />
				<span ui:field="groupDescribtion_3" /><br />
			</p>
			<p>
				<g:Label text="Founder:" />
				<g:Label ui:field="groupUser_3" /><br />
			</p>
		</p>
		
		<hr />
		
		<h4>Create new group</h4>
		
		<p>
			<p>
				<g:Label text="Title" />
				<g:TextBox ui:field="newGroupTitle" />
			</p>
			<p>
				<g:Label text="Description" />
				<g:TextArea ui:field="newGroupDescribtion" />
			</p>
			<p>
				<g:Button ui:field="newGroupButton" />
			</p>
		</p>
		
		<hr />
	
	</g:HTMLPanel>
	
</ui:UiBinder> 